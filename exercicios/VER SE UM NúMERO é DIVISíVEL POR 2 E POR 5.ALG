algoritmo "Ver se um número é divisível por 2 e 5"

var
n1:real

inicio
escreva("Digite um número ")
leia(n1)
se (n1 MOD 2 = 0) entao
   escreval(n1," é divisível por 2")
senao
     escreval(n1," não é divisível por 2")
fimse
se (n1 MOD 5 = 0) entao
   escreval(n1," é divisível por 5")
senao
     escreval(n1," não é divisível por 5")
fimse

fimalgoritmo